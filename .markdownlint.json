{
    "no-inline-html": {
        "allowed_elements": [
            "details",
            "center",
            "p",
            "i",
            "label",
            "div"
        ]
    },
    "first-line-h1": false,
    "single-h1": false,
    "siblings_only": true,
    "line-length": false,
    "no-space-in-code": false,
    "no-duplicate-header": false,
    "header-increment": false,
    "no-blanks-blockquote": false,
    "ol-prefix": false,
    "no-emphasis-as-header": false
}